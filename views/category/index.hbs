<div class="container col-md-6">
   <table>
      <tr>
         <th>Category</th>
         <th>Description</th>
         <th>Menu</th>
      </tr>
      {{#each categoryList }}
      <tr>
         <td>{{ name }}</td>
         <td>{{ description }}</td>
         <td>
           
            <a class="btn" href="/category/delete/{{ _id }}"
            onclick="return confirm('Do you want to delete this category ?');"
            >Delete</a>
         </td>
      </tr>
      {{/each}}
   </table>
</div>